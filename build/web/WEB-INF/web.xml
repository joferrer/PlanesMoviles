<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>ListarPlanes</servlet-name>
        <servlet-class>Control.ListarPlanes</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>SeleccionPlan</servlet-name>
        <servlet-class>Control.SeleccionPlan</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ServiciosAdicionales</servlet-name>
        <servlet-class>Control.ServiciosAdicionales</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>phone</servlet-name>
        <servlet-class>Control.phone</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Cotizacion</servlet-name>
        <servlet-class>Control.Cotizacion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>ListarPlanes</servlet-name>
        <url-pattern>/ListarPlanes</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>SeleccionPlan</servlet-name>
        <url-pattern>/SeleccionPlan</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ServiciosAdicionales</servlet-name>
        <url-pattern>/ServiciosAdicionales</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>phone</servlet-name>
        <url-pattern>/phone</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Cotizacion</servlet-name>
        <url-pattern>/Cotizacion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
